{
  "expo": {
    "name": "Rainy Partner App",
    "slug": "Rainy-Partner-App",
    "scheme": "Rainy-Partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Rainy-App-Icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Rainy-Filter-Logo-01.png",
      "resizeMode": "cover",
      "backgroundColor":"#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.rainyfilters.rainyplumber",
        "foregroundImage": "./assets/Rainy-Filter-Logo-01.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/Rainy_Filter_Logo.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
